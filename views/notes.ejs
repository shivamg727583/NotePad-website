 
 <%- include('./partials/head.ejs') %>





 <h1 class="text-3xl font-bold mb-4">Notes</h1>
 <% if (notes.length > 0) { %>
   <% notes.forEach(function(note) { %>
     <div class="note p-4 mb-4 rounded-md bg-white shadow-md">
       <h1 class="text-xl font-semibold text-blue-800"><%= note.title %></h1>
       <p class="mt-2"><%= note.content %></p>
       <div class="update flex justify-between items-center mt-2">
         <a href="/edit/<%= note._id %>" class="text-green-500">Edit</a>
         <a href="/delete/<%= note._id %>" class="text-red-600">Delete</a>
       </div>
     </div>
   <% }) %>
 <% } else { %>
   <h2 class="text-xl font-semibold mt-4">No more notes</h2>
 <% } %>
</div>
</body>

</html>
